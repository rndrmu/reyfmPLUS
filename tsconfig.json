{
    "compilerOptions": {
        "lib": ["es5", "es6", "dom", "ES2016"],
        "target": "ESNext",
        "rootDir": "./src/",
        "outDir": "./dist/pre/",
        "baseUrl": "./src/",
        "moduleResolution": "node",
        "paths": {
            "@utils/*": ["./utils/*"],
            "@api/*": ["./api/*"],
            "@webpack": ["./webpack/*"],
            "@core/*": ["./core/*"],
        }
    },
}